{
    "env": {
        "es6": true,
        "jquery": true
    },
    "rules": {
        "eqeqeq": 2,
        "camelcase": 1,
        "no-else-return": 1,
        "no-redeclare": 1,
        "no-mixed-spaces-and-tabs": 1,
        "no-console": 0,
        "no-empty": 2,
        "no-compare-neg-zero": 1,
        "no-cond-assign": 1,
        "no-constant-condition": 1,
        "no-control-regex": 1,
        "no-debugger": 1,
        "no-dupe-args": 1,
        "no-dupe-keys": 1,
        "no-duplicate-case": 1,
        "no-empty": 1,
        "no-empty-character-class": 1,
        "no-ex-assign": 1,
        "no-extra-boolean-cast": 1,
        "no-extra-semi": 1,
        "no-func-assign": 1,
        "no-inner-declarations": 1,
        "no-invalid-regexp": 1,
        "no-irregular-whitespace": 1,
        "no-obj-calls": 1,
        "no-regex-spaces": 1,
        "no-sparse-arrays": 1,
        "no-unexpected-multiline":1,
        "no-unreachable": 2,
        "no-unsafe-finally": 2,
        "no-unsafe-negation": 2,
        "use-isnan": 2,
        "valid-typeof": 1,
        "no-case-declarations": 1,
        "no-empty-pattern": 1,
        "no-fallthrough": 1,
        "no-global-assign": 1,
        "no-octal": 1,
        "no-self-assign": 1,
        "no-unused-labels": 1,
        "no-useless-escape": 1,
        "no-delete-var": 1,
        "no-class-assign": 2,
        "no-const-assign": 2,
        "no-dupe-class-members": 2,
        "no-new-symbol": 1,
        "brace-style": 2,
        "curly": 2
    }
}
